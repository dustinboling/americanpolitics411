<h1>Listing personal_assets</h1>

<table>
  <tr>
    <th>Person</th>
    <th>Organization name</th>
    <th>Action</th>
    <th>Date</th>
    <th>Value min</th>
    <th>Value max</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @personal_assets.each do |personal_asset| %>
  <tr>
    <td><%= personal_asset.person_id %></td>
    <td><%= personal_asset.organization_name %></td>
    <td><%= personal_asset.action %></td>
    <td><%= personal_asset.date %></td>
    <td><%= personal_asset.value_min %></td>
    <td><%= personal_asset.value_max %></td>
    <td><%= personal_asset.value %></td>
    <td><%= link_to 'Show', personal_asset %></td>
    <td><%= link_to 'Edit', edit_personal_asset_path(personal_asset) %></td>
    <td><%= link_to 'Destroy', personal_asset, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to '<< Back to People', people_path %>
